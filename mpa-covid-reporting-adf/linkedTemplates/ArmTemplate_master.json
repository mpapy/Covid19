{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mpa-covid-reporting-adf"
		},
		"GitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub1'"
		},
		"Is_ablob_covidreportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Is_ablob_covidreportingsa'"
		},
		"Is_adls_covidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Is_adls_covidreportingdl'"
		},
		"GitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "Marek"
		},
		"Is_adls_covidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mpacovidreportingdl.dfs.core.windows.net/"
		},
		"Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/8674e292-ebf6-450f-89c8-8e7df143bf56/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/mpacovidreportingsa"
		},
		"tr_ingest_population_data_1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/8674e292-ebf6-450f-89c8-8e7df143bf56/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/mpacovidreportingsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mpa-covid-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"GitHub1_credential": {
						"value": "[parameters('GitHub1_credential')]"
					},
					"Is_ablob_covidreportingsa_connectionString": {
						"value": "[parameters('Is_ablob_covidreportingsa_connectionString')]"
					},
					"Is_adls_covidreportingdl_accountKey": {
						"value": "[parameters('Is_adls_covidreportingdl_accountKey')]"
					},
					"GitHub1_properties_typeProperties_username": {
						"value": "[parameters('GitHub1_properties_typeProperties_username')]"
					},
					"Is_adls_covidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('Is_adls_covidreportingdl_properties_typeProperties_url')]"
					},
					"Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL')]"
					},
					"tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admissions_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName')]"
					},
					"tr_ingest_population_data_1_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}